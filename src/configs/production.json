{
    "env": "production",
    "external": {
        "mapServiceUrl": "https://nominatim.openstreetmap.org",
        "weatherServiceUrl": "https://api.openweathermap.org/"
    },
    "cache": {
        "ttl": 43200
    },
    "jwt": {
        "secretKey": "secretKey",
        "expiresIn": 36000
    },
    "log": {
        "format": "combined",
        "dir": "../logs"
    },
    "cors": {
        "origin": "your.domain.com",
        "credentials": true
    }
}